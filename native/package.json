{
    "name": "com.antonkluev.projector",
    "displayName": "Projector",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "dependencies": {
        "com.virtuoworks.cordova-plugin-canvascamera": "https://github.com/VirtuoWorks/CanvasCameraPlugin.git",
        "cordova-custom-config": "^3.2.0",
        "cordova-ios": "^4.4.0",
        "cordova-plugin-add-swift-support": "^1.6.2",
        "cordova-plugin-screen-orientation": "^2.0.1",
        "cordova-plugin-statusbar": "^2.2.3",
        "cordova-plugin-whitelist": "^1.3.2"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-custom-config": {},
            "cordova-plugin-add-swift-support": {},
            "cordova-plugin-screen-orientation": {},
            "cordova-plugin-statusbar": {},
            "com.virtuoworks.cordova-plugin-canvascamera": {
                "CAMERA_USAGE_DESCRIPTION": "Display video preview"
            }
        },
        "platforms": [
            "ios"
        ]
