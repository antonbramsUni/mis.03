{
    "name": "com.antonkluev.projector",
    "displayName": "Projector",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "scripts": {
        "client": "cross-env NODE_ENV=client.dev webpack-dev-server --config ./dev/webpack.js",
        "server": "cross-env NODE_ENV=server.dev webpack --config ./dev/webpack.js",
        "build": "cross-env NODE_ENV=build.prod webpack --config ./dev/webpack.js --progress"
    },
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "dependencies": {
        "com.virtuoworks.cordova-plugin-canvascamera": "https://github.com/VirtuoWorks/CanvasCameraPlugin.git",
        "cordova-custom-config": "^3.2.0",
        "cordova-ios": "^4.4.0",
        "cordova-plugin-add-swift-support": "^1.6.2",
        "cordova-plugin-iosrtc": "^4.0.1",
        "cordova-plugin-screen-orientation": "^2.0.1",
        "cordova-plugin-statusbar": "^2.2.3",
        "cordova-plugin-whitelist": "^1.3.2",
        "cross-env": "^5.0.0",
        "socket.io": "^2.0.1"
    },
    "devDependencies": {
        "babel-core": "^6.17.0",
        "babel-loader": "^6.2.4",
        "babel-preset-es2015-native-modules": "^6.9.0",
        "coffee-loader": "^0.7.2",
        "coffee-script": "^1.12.4",
        "css-loader": "^0.25.0",
        "file-loader": "^0.10.1",
        "loader-utils": "^1.0.3",
        "node-sass": "^3.11.2",
        "sass-loader": "^4.0.2",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "webpack": "^2.2.1",
        "webpack-dev-server": "^2.4.1"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-custom-config": {},
            "cordova-plugin-iosrtc": {},
            "cordova-plugin-add-swift-support": {},
            "cordova-plugin-screen-orientation": {},
            "cordova-plugin-statusbar": {},
            "com.virtuoworks.cordova-plugin-canvascamera": {
                "CAMERA_USAGE_DESCRIPTION": "Display video preview"
            }
        },
        "platforms": [
            "ios"
        ]
    }
}